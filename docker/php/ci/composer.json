{
    "require": {
        "ergebnis/composer-normalize": "2.42.0",
        "ergebnis/phpstan-rules": "2.2.0",
        "insolita/unused-scanner": "2.4.0",
        "maglnet/composer-require-checker": "4.11.0",
        "phpstan/phpstan": "1.11.2",
        "phpstan/phpstan-deprecation-rules": "1.2.0",
        "phpstan/phpstan-strict-rules": "1.6.0",
        "phpstan/phpstan-phpunit": "1.4.0",
        "phpstan/phpstan-symfony": "1.4.3",
        "spaze/phpstan-disallowed-calls": "3.4.0",
        "steevanb/php-code-sniffs": "4.5.1",
        "steevanb/php-parallel-processes": "0.13.0",
        "steevanb/php-collection": "5.0.1",
        "wapmorgan/php-deprecation-detector": "2.0.33"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
